<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<!-- 配置驱动 -->
	<classPathEntry
		location="D:\tool\mybatisgenecode\mysql-connector-java-5.1.18-bin.jar" />
	<context id="context1">
		<commentGenerator>
			<property name="suppressDate" value="true" />
			<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<!-- 数据库名称 账号 密码 驱动 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://114.215.154.9/billing?characterEncoding=utf-8"
			userId="root" password="topgroup" />
		<!-- 生成实体类 实体bean文件 -->
		<javaModelGenerator targetPackage="com.coderwood.bill.entity"
			targetProject="code" />
		<!-- mapper xml文件 -->
		<sqlMapGenerator targetPackage="com.coderwood.bill.mapper"
			targetProject="code" />
		<!-- mapper 接口文件 -->
		<javaClientGenerator targetPackage="com.coderwood.bill.mapper"
			targetProject="code" type="XMLMAPPER" />
		<!-- 数据库的表 -->
		<table schema="" tableName="billinfo" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="false"
			enableSelectByExample="false" selectByExampleQueryId="false"></table>
	</context>
</generatorConfiguration>